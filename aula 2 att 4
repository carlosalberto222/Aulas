#include <stdio.h>

int main() {
    float angulo1, angulo2, angulo3;

    // Entrada de dados
    printf("Digite o primeiro ângulo: ");
    scanf("%f", &angulo1);

    printf("Digite o segundo ângulo: ");
    scanf("%f", &angulo2);

    // Cálculo do terceiro ângulo
    angulo3 = 180.0 - (angulo1 + angulo2);

    // Saída formatada
    printf("O terceiro ângulo do triângulo é: %.0f graus\n", angulo3);

    return 0;
}
